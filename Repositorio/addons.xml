<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.fenix"
       name="[COLOR orangered][B]Fenix[/B][/COLOR]"
       version="0.0.1"
       provider-name="Equipo de kodiplex">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.liveresolver" version="0.1.24" optional="true"/>
    <import addon="script.module.youtube.dl" />
    <import addon="plugin.video.youtube"  />
    <import addon="script.module.urlresolver" />
    <import addon="script.module.simplejson" />
     <import addon="script.module.livestreamer" optional="true"/>
     <import addon="script.module.pyamf" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>

   <extension point="xbmc.addon.repository" name="Mi repositorio">
        <info>https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/</datadir>
    </extension>

  <extension point="xbmc.addon.metadata">
    <disclaimer>
	Addon Creado Por equipo Kodi Plex
	Base usada para la creacion del addon: FESTA-Creado por lourdes,picapau y equipo FESTA</disclaimer>
    <description>ESTE ADDON NO CONTIENE ENLACES PROPIOS,SON RECOPILACIONES DE LA RED, SOLO SE FACILITA EL ACESSO A ESAS WEBS, ESTAS WEBS SON LAS RESPONSABLES DE LOS CONTENIDOS</description>
    <platform>all</platform>
	  <assets>
		<icon>icon.png</icon>
		<fanart>fanart.jpg</fanart>
	  </assets>
  </extension>
</addon>

<addon id="repository.Duplex" name="[COLOR teal]Duplex[/COLOR] Repo" version="0.0.1" provider-name="Kodiplex">
    <extension point="xbmc.addon.repository" name="Mi repositorio">
        <info>https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/kodiplexkodi/REPO-DUPLEX/master/Repositorio/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repo</summary>
        <description></description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>
</addons>
